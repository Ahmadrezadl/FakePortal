			sponsorunivshare = 10;
			sponsorexecshare = (100 - sponsorsupvshare - sponsoroverhead > 0) ? (100 - sponsorsupvshare - sponsoroverhead) : 0;
		}
	}
	else if (sponsoroverheadcode == 7602){
		if (isnew == 1){
			sponsoroverhead = 30;
			if (creatorcode == 7802){
				sponsorunivshare = 25;
				if (place1type == 1){
					sponsoroverhead = 27.5;
